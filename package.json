{
  "name": "botdiscord",
  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "start": "pm2 start dist/index.js --name botdiscord",
    "stop": "pm2 stop botdiscord",
    "restart": "pm2 restart botdiscord",
    "deploy": "npm run build && pm2 reload botdiscord",
    "status": "pm2 status botdiscord",
    "monitor": "pm2 monit",
    "logs": "pm2 logs botdiscord --lines 1000"
  },
  "dependencies": {
    "discord.js": "^14.25.1",
    "dotenv": "^16.5.0",
    "node-cron": "^4.2.1",
    "pm2": "^6.0.14"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
